<div class="jumbotron header">
  <div class="container">
    <h1 id="bookings-h1"><%= current_user.first_name.capitalize! %>, here are your bookings</h1>
  </div>
</div>
<div class="content">
    <div class="container">
          <div class="card-product">
            <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/skateboard.jpg" />
            <div class="card-product-infos">
              <h2></h2>
                <p class="card-text">
                <strong>Date:</strong> <%= booking.from_date.strftime('%Y-%m-%d') %> to <%= booking.to_date.strftime('%Y-%m-%d') %><br>
                <strong>Yacht:</strong> <%= booking.yacht.name %><br>
              </p>
            </div>
          </div>
    </div>
</div>
